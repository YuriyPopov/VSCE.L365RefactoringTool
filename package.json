{
	"name": "refactoring-tool-l365",
	"displayName": "Refactoring Tool L365",
	"description": "A VS Code extension for performing refactoring for L365 project",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:al"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "refactoring-tool-l365.action-tooltip",
				"title": "L365: Set action tooltips for the current workspace"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
